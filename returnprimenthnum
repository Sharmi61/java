import java.io.*;
import  java.util.*;

// Read only region start
class UserMainCode
{

	public int NthPrime(int input1){
		// Read only region end
int i=2,j=2,count=2,p=0,in=0;
for(i=2;i<=count;i++)
{count++;
 j=2;
	while(j<=i-1)
	{
		if(i%j==0)
			break;
		j++;
		
	}if(i==j)
		p++;
 if(p==input1)
 { in=i;
  break;}
 
}return in;}
public static void main(String args[]){
UserMainCode u=new UserMainCode();
System.out.println(u.NthPrime(5);}
		
}
